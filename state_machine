// State Machine
digraph {
	Idle -> Processing [label=start_processing]
	Idle -> SuccessState [label=jump_to_success]
	Processing -> SuccessState [label="complete_ok
complete_ok2"]
	Processing -> ErrorState [label=fail]
	Idle [label=Idle]
	Processing [label=Processing]
	SuccessState [label=Success]
	ErrorState [label=Success]
}
